{
    "name": "ss-5-tcn-sm",
    "job_dir": "./results/ss-5-tcn-sm",
    "ds_path": "./datasets/store/fs-w-pa-14-60/mesa",
    "dataset": {
        "name": "hdf5",
        "params": {
            "feat_key": "features",
            "label_key": "stage_labels",
            "mask_key": "mask"
        }
