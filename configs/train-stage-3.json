            {"filters": 40, "depth": 2, "ddepth": 1, "kernel": [1, 5], "pool": [1, 2], "strides": [1, 2], "skip": true, "expand_ratio": 1, "se_ratio": 1, "dropout": 0, "norm": "layer"}
